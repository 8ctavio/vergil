{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleDetection": "force",
		"moduleResolution": "bundler",
		"verbatimModuleSyntax": true,
		"strict": true,
		"erasableSyntaxOnly": true,
		"noUncheckedIndexedAccess": true,
		"noImplicitOverride": true,
	}
}