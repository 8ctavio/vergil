{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleDetection": "force",
		"moduleResolution": "bundler",
		"declaration": true,
		"emitDeclarationOnly": true,
		"outDir": "./types/dist",
		"declarationMap": true,
		"strict": true,
		"erasableSyntaxOnly": true,
		"noUncheckedIndexedAccess": true,
		"noImplicitOverride": true,
	},
	"include": ["**/*.js"]
}