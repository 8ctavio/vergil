{
	"compilerOptions": {
		"checkJs": true,
		"target": "ESNext",
		"moduleResolution": "bundler",
		"moduleDetection": "force",
		"declaration": true,
		"emitDeclarationOnly": true,
		"strict": true,
		"erasableSyntaxOnly": true,
		"noUncheckedIndexedAccess": true,
		"noImplicitOverride": true,
	}
}