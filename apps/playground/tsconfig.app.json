{
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
		"module": "esnext",
		"moduleResolution": "bundler",
        "noEmit": true,
        "paths": {
            "@/*": ["./src/*"]
        },
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    },
    "include": [
        "env.d.ts",
        "src/**/*",
        "src/**/*.vue",
    ]
}